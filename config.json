{
  "num_epochs": 5,
  "batch_size": 128,
  "num_folds": 20,
  "loss": {
    "class_name": "CategoricalCrossentropy",
    "config": {
      "reduction": "auto",
      "name": "categorical_crossentropy",
      "from_logits": false,
      "label_smoothing": 0,
      "axis": -1
    }
  },
  "optimizer": {
    "class_name": "Custom>Adam",
    "config": {
      "name": "Adam",
      "weight_decay": 0.001,
      "clipnorm": null,
      "global_clipnorm": null,
      "clipvalue": null,
      "use_ema": false,
      "ema_momentum": 0.99,
      "ema_overwrite_frequency": null,
      "jit_compile": true,
      "is_legacy_optimizer": false,
      "learning_rate": 0.001,
      "beta_1": 0.9,
      "beta_2": 0.999,
      "epsilon": 1e-07,
      "amsgrad": false
    }
  },
  "metrics": [
    "CategoricalAccuracy", "F1Score"
  ]
}
